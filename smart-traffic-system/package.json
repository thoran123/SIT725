{
  "name": "smart-traffic-system",
  "version": "1.0.0",
  "description": "A smart traffic system application",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "start": "node index.js",
    "dev": "nodemon index.js"
  },
  "keywords": [
    "traffic",
    "smart",
    "system",
    "automation",
    "iot"
  ],
  "author": "thorancherukuru",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": ["<rootDir>/src", "<rootDir>/test"],
    "moduleDirectories": ["node_modules", "src"],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/node_modules/**",
      "!**/test/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": ["text", "lcov", "html"]
  }
}