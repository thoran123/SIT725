{
    "name": "sit725-applied-software-engineering-docker",
    "version": "1.0.0",
    "description": "SIT725 Applied Software Engineering Globe Visualization Project - Professional Docker Implementation by Thoran Kumar Cherukuru Ramesh",
    "main": "server.js",
    "scripts": {
      "start": "node server.js",
      "dev": "nodemon server.js",
      "docker:build": "docker build -t sit725-thoran-app:latest .",
      "docker:run": "docker run -d -p 3000:3000 --name sit725-thoran-container sit725-thoran-app:latest",
      "docker:stop": "docker stop sit725-thoran-container && docker rm sit725-thoran-container",
      "docker:compose:up": "docker-compose up -d",
      "docker:compose:down": "docker-compose down",
      "docker:logs": "docker logs sit725-thoran-container -f",
      "docker:clean": "docker system prune -a",
      "test": "npm run test:docker",
      "test:docker": "docker build -t sit725-test . && docker run --rm sit725-test npm test",
      "health:check": "curl -f http://localhost:3000/api/health || exit 1"
    },
    "keywords": [
      "SIT725",
      "Applied Software Engineering",
      "Docker",
      "Express",
      "Globe Visualization",
      "Client-side Application",
      "Containerization"
    ],
    "author": "Thoran Kumar Cherukuru Ramesh <s224967779@deakin.edu.au>",
    "license": "MIT",
    "repository": {
      "type": "git",
      "url": "https://github.com/thoran123/SIT725_project.git"
    },
    "dependencies": {
      "express": "^4.18.2",
      "cors": "^2.8.5",
      "helmet": "^7.0.0",
      "compression": "^1.7.4",
      "dotenv": "^16.3.1"
    },
    "devDependencies": {
      "nodemon": "^3.0.1"
    },
    "engines": {
      "node": ">=18.0.0",
      "npm": ">=8.0.0"
    }
  }